<style>    
.columns .bot-img {
    box-shadow: 1px 1px 1px 1px transparent !important;
    animation: pulse 2s infinite;
    border-radius: 50%;
    }
    ::-moz-selection { /* Code for Firefox */
    color: #3191ff;
    background: #00000040;
    border-radius: 15px;
    }
    ::selection {
    color: #3191ff;
    background: #00000040;
    }
    ::-webkit-scrollbar {
    width: 9px;
    }
    ::-webkit-scrollbar-thumb {
    background-color: #18191c;
    border-radius: 8px;
    }
    ::-webkit-scrollbar-track {
    background-color: #18191c00;
    }
  </style>
<body class="hold-transition sidebar-mini layout-fixed">
<div class="wrapper">

  <%- include ("templates/navigation.ejs") %>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <% if(maintenanceStatus == false){ %>
                <h1 class="m-0 text-dark">Управление</h1>
                <% }else{ %>
                    <h1 class="m-0 text-dark">Управление <span class="maintenanceActive">Активно обслуживание!</span></h1>
                <% } %>
          </div><!-- /.col -->
        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->

    <!-- Main content -->
    <main class="mdl-layout__content">
        <!-- page content -->
        <div class="page-content">
            <div class="mdl-grid">
                <div class="mdl-cell mdl-cell--2-col"></div>
                <div class="mdl-cell mdl-cell--8-col">
                    <h1><b>Логи</b></h1>
                    <hr>
                    <div class="logContainer">
                        <ul>
                            <% log.forEach((log) => {%>
                                <li class="<%= "log_" + log.log_type %>">
                                    <%

                                        let convertingGetDay = function (getDay) {
                                            let day;
                                            switch (getDay){
                                                case 0:
                                                    day = "Воскресенье";
                                                    break;
                                                case 1:
                                                    day = "Понедельник";
                                                    break;
                                                case 2:
                                                    day = "Вторник";
                                                    break;
                                                case 3:
                                                    day = "Среда";
                                                    break;
                                                case 4:
                                                    day = "Четверг";
                                                    break;
                                                case 5:
                                                    day = "Пятница";
                                                    break;
                                                case 6:
                                                    day = "Суббота";
                                                    break;
                                                default:
                                                    day = "Возникла проблема при попытке преобразовать целое число Date.getDay () в строку \n";
                                            }
                                            return day;
                                        };

                                        let date = new Date(log.log_date);
                                        let day = convertingGetDay(date.getDay());

                                        // To understand converting timestamps to a normal known date
                                        // look at this question in StackOverflow -> https://goo.gl/Lb2Nxa
                                        // MDN Documentation about Date -> https://goo.gl/rT25GW

                                        // Minutes part from the timestamp
                                        let minutes = "0" + date.getMinutes();
                                        // Seconds part from the timestamp
                                        let seconds = "0" + date.getSeconds();

                                    %>
                                    [<%= day + ", " + date.getMonth() +  "/" + date.getDate() + "/" + date.getFullYear() + ", " + date.getHours() + ':' + minutes.substr(-2) + ":" + seconds.substr(-2) %>]
                                    <b><%= log.log_message %></b><br>
                                    <small><%= log.log_action %></small>

                                </li>
                            <% });%>
                        </ul>
                    </div>
                </div>
                <div class="mdl-cell mdl-cell--2-col">
                </div>
            </div>
        </div>
    </main>
</div>
<div class="mdl-cell mdl-cell--2-col">
</div>
    <!-- /.content -->
  </div>

  <!-- Control Sidebar -->
  <aside class="control-sidebar control-sidebar-dark">
    <!-- Control sidebar content goes here -->
  </aside>
  <!-- /.control-sidebar -->
</div>
<!-- ./wrapper -->
</div>
</div>
</main>
<div class="row">

</div>
<!-- /.row (main row) -->
</div><!-- /.container-fluid -->
</section>
<!-- /.content -->
</div>

